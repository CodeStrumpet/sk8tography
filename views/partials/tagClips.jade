h3 Tag Clips Eh

div.container-fluid
	div.sidebar.left
		div.well
			h5 Sidebar
			ul.nav.nav-list
				li(ng-repeat="clip in clips", ng-class="{activeclip: $index==currClipIndex}", ng-click="setCurrentClip(clip)", clips-loaded)
					img(ng-src="http://img.youtube.com/vi/{{clip.videoId}}/default.jpg")
					p {{clip.videoId}}
	div.sidebar.right
		div.well
			h5 Sidebar
	div.content
		div.hero-unit
			h1 Content
			div(ng-controller='YoutubeCtrl')
				include includes/videoconsole